<h2> <%= t( "models.event.name" ).pluralize %> </h2>

<ul id="event_list" >
  <% @events.each do |event| %>
    <li class="event" id="event_#{event.id}">
      <%= render partial: 'event', :locals => { :event => event } %>
    </li>
  <% end %>
</ul>

<%= link_to t( "application.links.back_to_sports" ), sports_path %> 